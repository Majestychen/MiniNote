<!--pages/update/index.wxml-->
<view class="container">
  <scroll-view scroll-y="true"> 
    <view class="tr" wx:for="{{updateHistoryData}}" wx:key="time" >
      <text>{{item.time}}</text>
      <view>
        <text>{{item.content}}</text>
        <text>\n{{item.content1}}</text>
        <text>\n{{item.content2}}</text>
      </view>
    </view>
  </scroll-view>
</view>